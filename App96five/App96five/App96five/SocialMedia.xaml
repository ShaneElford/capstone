<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App96five.SocialMedia">
    <ContentPage.Content>
        <StackLayout>
            <Button BackgroundColor="Maroon" TextColor="White" FontSize="Large" Text="Facebook" Clicked="GoToFacebook"/>
            <Button Text="Instagram" Clicked="GoToInstagram"/>
            <Button Text="Twitter" Clicked="GoToTwitter"/>
            <Button Text="YouTube" Clicked="GoToYoutube"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>